#include<stdio.h>
int main ()
{
	int a,b,c;
	printf("enter a,b,c values");
	scanf("%d%d%d",&a,&b,&c);
	if(a>b)
	{
		if(a>c)
		printf("a is big");
		else
		printf("b is big");
	}
	else{
		if(b>c)
		printf("b is big");
		else
		printf("c is big");
	}
	return 0;
}
output:enter a,b,c values
25 26 27
c is big
