#include<stdio.h>
int main()
{
	int n,i,j,temp,a[100];
	printf("Enter array size");
	scanf("%d",&n);
	printf("Enter array value");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=j+1;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			if(a[i]>a[j])
			{
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
	printf("\n select array values :");
	for(i=0;i<n;i++)
	{
		printf("%d",a[i]);
	}
	return 0;
}
output:Enter array size
2
Enter array value
2
2

 select array values :22
